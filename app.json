{
  "healthchecks": {
    "web": [
      {
        "type": "startup",
        "name": "web check",
        "initialDelay": 10,
        "description": "Checking if the app responds to the /up endpoint",
        "path": "/up",
        "attempts": 3
      }
    ]
  },
  "cron": [
    {
      "command": "bin/rails sidekiq:check_health",
      "schedule": "*/10 * * * *"
    }
  ]
}
