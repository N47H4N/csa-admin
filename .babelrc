{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": ["> 0.5%", "ie >= 11", "safari >= 8"]
        },
        "uglify": true,
        "useBuiltIns": true
      }
    ]
  ],

  "plugins": [
    "syntax-dynamic-import",
    "transform-object-rest-spread",
    ["transform-class-properties", { "spec": true }]
  ]
}
